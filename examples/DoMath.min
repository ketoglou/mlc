/*In this program we give user some options we let him insert two number and then do one of the 4 (+,-,*,/)*/


program doMath
{
      declare num1,num2,selection;
      
      forcase
            when(selection=0):print(num1+num2)
            when(selection=1):print(num1-num2)
            when(selection=2):print(num1 * num2)
            when(selection=3):print(num1 / num2)
            default:print(0)
}
